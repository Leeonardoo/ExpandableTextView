<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableTextView">
        <attr name="etv_expandLines" format="integer" />
        <attr name="etv_shrinkBitmap" format="reference" />
        <attr name="etv_expandBitmap" format="reference" />
        <attr name="etv_textStateColor" format="color" />
        <attr name="etv_textContentColor" format="color" />
        <attr name="etv_textContentSize" format="dimension" />
        <attr name="etv_textShrink" format="string" />
        <attr name="etv_textExpand" format="string" />
        <attr name="etv_animationTime" format="integer" />
        <attr name="etv_showLine" format="boolean" />
    </declare-styleable>
</resources>